                            {
                                "Widget": "/Ark-Nucleus/SharedContent/Widgets/Views/DU_View_SimpleText_v1.DU_View_SimpleText_v1_C",
                                "Props": {
                                    "Text": "1. Trading for Real Money",
                                    "FontSize": 18,
                                    "Color": [0.3, 0.8, 1],
                                    "Padding": [5, 10, 5, 5]
                                }
                            },
                            {
                                "Widget": "/Ark-Nucleus/SharedContent/Widgets/Views/DU_View_SimpleText_v1.DU_View_SimpleText_v1_C",
                                "Props": {
                                    "Text": "No trading any in-game resource, items or tames for real life money.",
                                    "FontSize": 13,
                                    "Padding": [15, 5, 5, 15]
                                }
                            },
                            {
                                "Widget": "/Ark-Nucleus/SharedContent/Widgets/Views/DU_View_SimpleText_v1.DU_View_SimpleText_v1_C",
                                "Props": {
                                    "Text": "2. Spawn Points, Rare Resources, Caves & POIs",
